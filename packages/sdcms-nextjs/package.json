{
  "name": "@sdcms/nextjs",
  "version": "0.3.0",
  "description": "Next.js SDK for SDCMS headless CMS integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepare": "tsup",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cms",
    "headless",
    "nextjs",
    "react",
    "seo"
  ],
  "author": "SDCMS",
  "license": "MIT",
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "swr": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "swr": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "swr": "^2.4.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
